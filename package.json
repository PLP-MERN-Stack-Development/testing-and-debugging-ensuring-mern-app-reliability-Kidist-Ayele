{
  "name": "mern-testing-assignment",
  "version": "1.0.0",
  "description": "MERN stack application with comprehensive testing",
  "scripts": {
    "install-all": "cd server && npm install && cd ../client && npm install",
    "test": "jest --config=jest.config.js",
    "test:unit": "jest --config=jest.config.js --testPathPattern='unit'",
    "test:integration": "jest --config=jest.config.js --testPathPattern='integration'",
    "test:coverage": "jest --coverage --config=jest.config.js",
    "test:e2e": "cd client && npx cypress run"
  },
  "keywords": [
    "mern",
    "testing",
    "jest",
    "cypress"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "babel-jest": "^30.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0"
  }
}
